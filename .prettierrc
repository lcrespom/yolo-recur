{
  "semi": false,
  "singleQuote": true,
  "useTabs": false,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "printWidth": 90,
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "printWidth": 90,
        "proseWrap": "always"
      }
    }
  ]
}
